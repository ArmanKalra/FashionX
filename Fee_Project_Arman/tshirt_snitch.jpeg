  async function parseData(data, tabContentId) {
    var responseJson = $.parseJSON(data);
    var resultArray = responseJson.result;
    // console.log('resultArray::',resultArray);
    let mainHtml = ``;
   // console.log("Parsing : ", resultArray);
    function processResultArray(resultArray) {
    const promises = resultArray.map((ob, index) => {
        return new Promise((resolve) => {
            const html = `
                <div data-href-ss="${ob.data.product_url}" data-handle="${ob.data.product_url.split('/').pop()}" data-id-ss="${ob.product_id}" data-index-ss="${index + 1}" class="tw-card-find ss-item-main-div">
