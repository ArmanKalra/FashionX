  function handleIntersection (entries){
    entries.forEach((entry) => {
        if (entry.isIntersecting) {
          if(!data_ss_id.includes(entry.target.getAttribute("data-id-ss"))){
          //  console.log("Element in viewport:", entry.target, entry.target.getAttribute("data-id-ss"));
            var pid = entry.target.getAttribute("data-id-ss");
            var pos = entry.target.getAttribute("data-index-ss");
            var queryID = 8461940818082;
             SimilarStyles.sendEvent("product_view", {
               queryId: localStorage.getItem("visenze_queryId_ss"), 
               pid: pid, // product id of the search/recommendation result
               pos: pos, // product position in Search Results, start from 1
             });
         // console.log(pid,pos,queryID)  ;
