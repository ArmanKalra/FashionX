  function changeTab(tabId) {
    // Remove 'active' class from all tabs and tab contents
   console.log('inside change tab');
    const tabs = document.querySelectorAll('.tw-tab');
    tabs.forEach(tab => tab.classList.remove('tw-active'));

    const tabContents = document.querySelectorAll('.tw-tab-content');
    tabContents.forEach(content => content.style.display = 'none');
   
    const clickedTab = document.getElementById(tabId);
    const correspondingTabContent = document.getElementById(`tw-tabContent${tabId.charAt(tabId.length - 1)}`);
    
    clickedTab.classList.add('tw-active');
    correspondingTabContent.style.display = 'flex';
    correspondingTabContent.innerHTML = '<div class="loading-placeholder"></div>';
    if (tabId === 'tw-tab1') {
        getData(product_id, 'tabContent1');
    } else if (tabId === 'tw-tab2') {
        getData1(product_id, 'tabContent2');     
    }
}

