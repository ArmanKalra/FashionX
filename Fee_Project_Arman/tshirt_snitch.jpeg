  function sendGAEvent(arr, tab) {
    var result = arr.result;
    // console.log('tab is :',tab,result);
    ecommerce.items = [];
    
    // Array to store all fetch promises
    var fetchPromises = [];

    result.forEach((item, index, array) => {
        var lastElement = item.data.product_url.split('/').pop();
        // Push each fetch promise to the array
        fetchPromises.push(
            fetch(`/products/${lastElement}.js`)
            .then(response => response.json())
            .then(data => {
                var jsonData = data; // Store the JSON data in a variable
                var newItem = {
                    'item_id': jsonData.id,
                    'item_name': jsonData.title.replace(/['"]/g, ''),
                    'affiliation': jsonData.vendor.replace(/['"]/g, ''),
                    'coupon': '',
                    'discount': '',
                    'index': index,
                    'item_brand': jsonData.vendor.replace(/['"]/g, ''),
                    'item_variant': jsonData.variants[0].public_title.replace(/['"]/g, ''),
                    'item_category': '',
                    'item_category2': '',
                    'item_category3': '',
                    'sku': jsonData.variants[0].sku,
                    'item_list_id': `Visenze ${tab}`,
                    'item_list_name': `Visenze ${tab}`,
                    'item_size': jsonData.variants[0].options[1],
                    'price': (jsonData.price * 0.01).toFixed(2),
                    'quantity': 1
                };
                ecommerce.items.push(newItem); // Push the new item into the ecommerce items array
            })
            .catch(error => console.error('Error fetching JSON:', error))
        );
    });

    // Wait for all fetch promises to resolve
    Promise.all(fetchPromises)
    .then(() => {
        // Push the dataLayer once all fetch requests are completed
        dataLayer.push({
            'pageType': 'Product Page',
            'event': 'view_item_list',
            'currency': 'INR',
            ecommerce
        });
    });
}

