            matchAndHide(cartItemIds);
        });
      }
    });
  }
  function matchAndHide(arr) {
      let ids = ["","43613251600546","43613248520354","43613245112482","43613237543074","43611230666914","44747958059170","44747958878370"];
      ids.shift();
      let idSet = new Set(ids);
      // console.log('Matched elements are : ',arr,idSet);
      arr.forEach(element => {
          if (idSet.has(element)) {
              // console.log('Match Found!',element);
            $(`#recom-product-${element}`).hide();
          }
      });
  }
  function getCart(callback) {
      $.ajax({
          type: 'GET',
          url: '/cart.js',
          cache: false,
          dataType: 'json',
          success: function(cart) {
              var productIds = [];
              if (cart && cart.items) {
                  productIds = cart.items.map(function(item) {
                      return item.id.toString();
                  });
              }
              // Call the callback function with the cart items
              callback(productIds);
          }
      });
  }
