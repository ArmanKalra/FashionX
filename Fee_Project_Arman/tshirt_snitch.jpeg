      let currentProduct = await getRecommendedProductUsingHandle(item);
      pushHTML(currentProduct);
    });
    getCart(function(cartItemIds) {
        // console.log('Cart Items are :', cartItemIds);
        matchAndHide(cartItemIds);
    });
  });
