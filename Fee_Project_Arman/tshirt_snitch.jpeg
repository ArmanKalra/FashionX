                        <img src="${ob.main_image_url}">
                        <span class="similarProductTitle">${ob.data.title}</span>
                        <span class="similarProductPrice">Rs. ${ob.data.price.value}</span>
                    </div>
                </div>
            `;
            resolve(html);
        });
    });

    return Promise.all(promises).then((htmlArray) => {
        return htmlArray.join('');
    });
