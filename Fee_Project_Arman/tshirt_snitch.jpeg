<script>
document.querySelector('.js-drawer-open-cart').addEventListener('click', function() {
  $.ajax({
      type: 'GET',
      url: '/cart.js',
      cache: false,
      dataType: 'json',
      success: function(cart) {
        console.log('This is the cart !',cart);
        var ecommerce = {
            'value': (cart.total_price / 100.0).toFixed(2),
            'items': []
        };
        for (var i = 0; i < cart.items.length; i++) {
            var line_item = cart.items[i];
            var collections = document.querySelectorAll('.cart__item')[i].getAttribute('data-collections').split('-');
            // console.log('collections are :',collections);
            // var curr = `'currency': '${cart.currency}'`;
            // if(i==0){
            //   ecommerce.items.push(curr);
            // }
            var item = {
                'item_id': line_item.id,
                'item_name': line_item.product_title.replace(/['"]/g, ''),
                'affiliation': line_item.vendor.replace(/['"]/g, ''),
                'coupon': line_item.discounts.length,
                'discount': (line_item.original_price - line_item.discounted_price).toFixed(2),
                'index': i,
                'item_brand': line_item.vendor.replace(/['"]/g, ''),
                'item_category': collections[0],
                'item_category2': collections[1],
                'item_category3': collections[2],
                'item_variant': line_item.variant_title.replace(/['"]/g, ''),
                'item_list_id': 'product' + ' Page',
                'item_list_name': 'product' + ' Page',
                'item_size': line_item.variant_options[1],
                'sku': line_item.sku,
                'price': (line_item.price * 0.01).toFixed(2),
                'quantity': line_item.quantity
            };
        
            ecommerce.items.push(item);
        }
        var dataLayerObject = {
            'pageType': 'Cart',
            'event': 'view_cart',
            'currency': `${window.Shopify.currency.active}`,
            'ecommerce': ecommerce
        };
        
        dataLayer.push(dataLayerObject);

      }
  });
    
});
