    let constInter = setInterval(() => {
      let target = document.querySelector(".simpl-div");
      target.insertAdjacentHTML("beforebegin", html);
      clearInterval(constInter);
    }, 1000);
  };

  let splitCalculate = async (data) => {
    const div = data / 3;
    const pow = Math.pow(10, 2);
    const final = Math.round(div * pow) / pow;
    const str = String(final);
    if (str.includes(".")) return (final + 0.01).toFixed(2);
    else return final;
  };

  let cal = splitCalculate(priceRm);
  cal.then((value) => {
